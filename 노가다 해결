package adsfasdf;

import java.util.Scanner;

public class ASSDFasdf {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("수를 입력하시오 : ");
		int num = sc.nextInt();
		
		for(;num>0;) {
			System.out.println(num%10);
			num=num/10;
		}
		
		
		 
		
		
		                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
		

	}

}
